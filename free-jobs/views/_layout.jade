html
  head
    meta(http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate")
    meta(http-equiv="Pragma" content="no-cache")
    meta(http-equiv="Expires" content="0")
    
    title Ofício já
    
    meta(name="viewport" content="width=device-width, initial-scale=1")
    link(href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet")
    link(rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,400italic")
    
    block master_preCss
    
    link(rel="stylesheet" href="#{resources_root}/bower/material-design-lite/material.min.css")
    link(rel="stylesheet" href="#{resources_root}/bower/angular-data-grid/demo/material/css/angular-data-grid.material.css")
    link(rel="stylesheet" href="#{resources_root}/bower/angular-material-expander/dist/angular-material-expander.min.css")
    
    link(rel="stylesheet" href="#{resources_root}/bower/angular-material/angular-material.min.css")
    link(rel="stylesheet" href="#{resources_root}/bower/angular-material-icons/angular-material-icons.css")

    link(rel="stylesheet" href="#{resources_root}/materialize-custom.css")

    block master_css
    block master_posCss

  
  body(ng-app="sgtrApp")
    div(layout="column" flex="" layout-fill="" ng-cloak="")
    
      md-toolbar(class="md-hue-2" ng-controller="AppCtrl" md-scroll-shrink="md-scroll-shrink")
        div(class="md-toolbar-tools")
          md-button(ng-click="toggleLeft()" class="md-icon-button" aria-label="Abrir/Fechar menu")
            ng-md-icon(icon="menu"/)

          h2
            span SGTR
            small 
          span(flex="50px")
          
          h3
            span(hide-xs="") page_title
            span(hide-gt-xs="") page_subtitle
          
          span(flex="flex")


      div(flex="" layout="row")
        md-sidenav(class="md-sidenav-left" md-component-id="left" md-whiteframe="4" )
          md-toolbar(class="md-theme-indigo" ng-controller="LeftCtrl")
            h1(class="md-toolbar-tools" hide-xs="hide-xs") SGTR
            
            md-button(ng-click="close()" hide-gt-md="hide-gt-md" aria-label="Fechar menu")
              ng-md-icon(icon="menu")
          
          md-content(ng-controller="LeftCtrl")
            md-list(layout-fill="")
              md-subheader(ng-repeat-start="group in groups" class="md-no-sticky") 
              strong {{group.label}}
              md-list-item(ng-repeat="item in group.items" ng-click="open(item);" md-colors="{'background': (isActiveMenu(item) ? 'blue-grey': null)}")
                md-button(class="md-icon-button")
                  md-icon(class="material-icons" style="font-size: 21px") {{item.icon}}
                {{item.label}}

                span(flex="")

              md-divider(ng-repeat-end="")
    
    
        md-content(flex="" )
          ui:insert(name="master_main_content")
    


    block master_preScripts
    
    
    script(src="#{resources_root}/bower/jquery/dist/jquery.min.js" type="text/javascript")</script>
    script(src="#{resources_root}/bower/lodash/dist/lodash.min.js" type="text/javascript")</script>


    script(src="#{resources_root}/bower/linqjs/linq.min.js" type="text/javascript")</script>
    script(type="text/javascript")
      var from = null;
      $(function(){from = Enumerable.from;});
    </script>


    script(src="#{resources_root}/bower/moment/min/moment.min.js" type="text/javascript")</script>
    script(src="#{resources_root}/bower/moment/locale/pt-br.js" type="text/javascript")</script>

    !--(Angular Material requires Angular.js Libraries --)
    script(src="#{resources_root}/bower/angular/angular.min.js" type="text/javascript")</script>
    script(src="#{resources_root}/bower/angular-messages/angular-messages.min.js" type="text/javascript")</script>
    script(src="#{resources_root}/bower/angular-animate/angular-animate.min.js" type="text/javascript")</script>
    script(src="#{resources_root}/bower/angular-aria/angular-aria.min.js" type="text/javascript")</script>
    script(src="#{resources_root}/bower/angular-sanitize/angular-sanitize.min.js" type="text/javascript")</script>
    script(src="#{resources_root}/bower/angular-resource/angular-resource.min.js" type="text/javascript")</script>
    script(src="#{resources_root}/bower/angular-route/angular-route.min.js" type="text/javascript")</script>
    script(src="#{resources_root}/bower/angular-i18n/angular-locale_pt-br.js" type="text/javascript")</script>

    
    !--(Angular Material Library --)
    script(src="#{resources_root}/bower/angular-material/angular-material.min.js" type="text/javascript")</script>
    script(src="#{resources_root}/bower/angular-material-icons/angular-material-icons.min.js" type="text/javascript")</script>
    
    !--(Angular Material Plugins --)
    
    !--(http://angular-data-grid.github.io/demo/material/ --)
    !--(https://github.com/angular-data-grid/angular-data-grid.github.io --)
    script(src="#{resources_root}/bower/angular-data-grid/dist/pagination.min.js")</script>
    script(src="#{resources_root}/bower/angular-data-grid/dist/dataGrid.min.js")</script>
    
    script(src="#{resources_root}/bower/angular-material-expander/dist/angular-material-expander.min.js")</script>
    script(src="#{resources_root}/bower/clipboard/dist/clipboard.min.js")</script>
    script(src="#{resources_root}/bower/ngclipboard/dist/ngclipboard.min.js")</script>

    script(src="#{resources_root}/String.prototype.js")</script>
    
    script(src="#{app_root}/app.js" type="text/javascript")</script>
    script(src="#{app_root}/_layout/controller-sidenav.js" type="text/javascript")</script>

    ui:insert(name="master_scripts"/)
    
    block master_posScripts
  
    
  </body>
</f:view>
</html>



   
